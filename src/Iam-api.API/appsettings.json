{
	"Kestrel": {
		"Limits": {
			"MaxConcurrentConnections": 300,
			"MaxConcurrentUpgradedConnections": 300
		},
		"DisableStringReuse": true,
		"Endpoints": {
			"Http": {
				"Url": "http://*:5000"
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*"
}
